  "scripts": {
    "prepare": "npx husky install",
    "start": "nx serve eodh-fe",
    "build": "nx build eodh-fe --configuration=production",
    "build:storybook": "nx build-storybook storybook-host --configuration=production",
    "start:storybook": "nx storybook storybook-host",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "nx run eodh-fe-e2e:e2e",
    "test:unit": "nx run-many --target=test --all --parallel --pass-with-no-tests --skip-nx-cache",
    "test:storybook": "nx test-storybook storybook-host",
    "test:storybook:ci": "nx run storybook-host:test-storybook-ci",
    "lint:check": "nx run-many --all --target=lint --parallel",
    "lint:fix": "nx run-many --all --target=lint --fix --parallel",
    "format:fix": "nx format:write --configuration ./.prettierrc",
    "format:check": "nx format:check --configuration ./.prettierrc",
    "code:check": "npm run format:check && npm run lint:check",
    "code:fix": "npm run format:fix && npm run lint:fix",
    "docker:build": "docker-compose up",
    "docker:rebuild": "docker-compose up --build",
    "create-lib": "nx g @spyrosoft/spyro-architecture-plugin:create-lib",
    "group": "nx g @spyrosoft/spyro-architecture-plugin:group",
    "translation:extract": "i18next --config apps/eodh-fe/i18next-parser.config.js",
    "translation:type:generate": "i18next-resources-for-ts interface -i ./apps/eodh-fe/public/assets/i18n -o @types/i18next/resources.d.ts",
    "config:create": "node ./scripts/create-config-file.js"
  },
