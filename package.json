{
  "name": "@ukri-fe/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "npx husky install",
    "start": "nx serve eodh-fe",
    "build:dev": "nx build eodh-fe --configuration=development",
    "build:prod": "nx build eodh-fe --configuration=production",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "nx run eodh-fe-e2e:e2e",
    "test:unit": "nx run-many --target=test --all --parallel --skip-nx-cache",
    "lint:check": "nx run-many --all --target=lint --parallel",
    "lint:fix": "nx run-many --all --target=lint --fix --parallel",
    "format:fix": "nx format:write --configuration ./.prettierrc",
    "format:check": "nx format:check --configuration ./.prettierrc",
    "code:check": "npm run format:check && npm run lint:check",
    "code:fix": "npm run format:fix && npm run lint:fix",
    "docker:build": "docker-compose up",
    "docker:rebuild": "docker-compose up --build",
    "create-lib": "nx g @spyrosoft/spyro-architecture-plugin:create-lib",
    "group": "nx g @spyrosoft/spyro-architecture-plugin:group"
  },
  "private": true,
  "dependencies": {
    "@swc/helpers": "~0.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@nx/cypress": "16.8.1",
    "@nx/eslint-plugin": "16.8.1",
    "@nx/js": "16.8.1",
    "@nx/linter": "16.8.1",
    "@nx/react": "16.8.1",
    "@nx/vite": "16.8.1",
    "@nx/workspace": "16.8.1",
    "@spyrosoft/eslint-config-spyro": "latest",
    "@spyrosoft/spyro-plugin-manager": "2.1.3",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.14.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-is": "18.2.1",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "~4.0.0",
    "@vitest/coverage-c8": "~0.32.0",
    "@vitest/ui": "~0.32.0",
    "babel-plugin-styled-components": "1.10.7",
    "cypress": "^13.0.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jsdom": "~22.1.0",
    "nx": "16.8.1",
    "prettier": "^2.6.2",
    "typescript": "~5.1.3",
    "vite": "~4.3.9",
    "vitest": "~0.32.0"
  }
}
